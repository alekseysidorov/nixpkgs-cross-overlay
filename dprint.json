{
  "json": {},
  "markdown": {},
  "toml": {},
  "dockerfile": {},
  "rustfmt": {
    "edition": "2021",
    "newline_style": "Unix",
    "unstable_features": true,
    "group_imports": "StdExternalCrate",
    "imports_granularity": "Crate"
  },
  "includes": [
    "**/*.{json,md,toml,dockerfile,rs,proto,js}"
  ],
  "excludes": [
    ".embuild",
    "**/*-lock.json",
    "**/target/**"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.15.5.wasm",
    "https://plugins.dprint.dev/markdown-0.14.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/dockerfile-0.2.3.wasm",
    "https://plugins.dprint.dev/rustfmt-0.6.1.exe-plugin@99b89a0599fd3a63e597e03436862157901f3facae2f0c2fbd0b9f656cdbc2a5"
  ]
}
